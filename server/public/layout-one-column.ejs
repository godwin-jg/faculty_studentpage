<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <title>KEC</title>
    <base href="/" />
    <link rel="icon" type="image/png" sizes="16x16" href="https://placement.kongu.edu/assets/favicons/192.png">
    <link href="css/style.css" rel="stylesheet">

</head>

<body>

    <!--*******************
        Preloader start
    ********************-->
    <div id="preloader">
        <div class="loader">
            <svg class="circular" viewBox="25 25 50 50">
                <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="3" stroke-miterlimit="10" />
            </svg>
        </div>
    </div>
    <!--*******************
        Preloader end
    ********************-->


    <!--**********************************
        Main wrapper start
    ***********************************-->
    <div id="main-wrapper">


        <!--**********************************
            Header start
        ***********************************-->
        <div class="header">
            <div class="header-content clearfix">

                <div class="nav-control">
                    <div class="hamburger">
                        <span class="toggle-icon"><i class="icon-menu"></i></span>
                    </div>
                </div>
                <div class="header-left">
                    <div class="input-group icons">
                        <div class="input-group-prepend">
                            <img src="https://placement.kongu.edu/assets/favicons/192.png" alt="" height="63px"
                                width="65" style="margin-top: -16.5px; border-style: ridge;">
                        </div>
                        <H2 style="padding-left: 13px;">KEC</H2>
                    </div>
                </div>
                <div class="header-right">
                    <ul class="clearfix">
                        <li class="icons dropdown">
                            <div class="user-img c-pointer position-relative" data-toggle="dropdown">
                                <span class="activity active"></span>
                                <img src="images/user/1.png" height="40" width="40" alt="">
                            </div>
                            <div class="drop-down dropdown-profile animated fadeIn dropdown-menu">
                                <div class="dropdown-content-body">
                                    <ul>
                                        <li><a href="/login"><i class="icon-key"></i> <span>Logout</span></a>
                                        </li>
                                    </ul>
                                </div>
                            </div>

                        </li>
                        <li class="icons dropdown d-none d-md-flex">
                            <h5 style="margin-top: 10px;"><span>
                                    <%=email %>
                                </span></h5>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <!--**********************************
            Header end ti-comment-alt
        ***********************************-->

        <!--**********************************
            Sidebar start
        ***********************************-->
        <div class="nk-sidebar">
            <div class="nk-nav-scroll">
                <ul class="metismenu" id="menu">
                    <li class="nav-label">Dashboard</li>
                    <li>
                        <a href="/api/courses/getcourses">
                            <i class="icon-speedometer menu-icon"></i><span class="nav-text">Home</span>
                        </a>
                    </li>
                    <li>
                        <a href="/">
                            <i class="icon-note menu-icon"></i><span class="nav-text">My Courses</span>
                        </a>
                    </li>
                </ul>
            </div>
        </div>
        <!--**********************************
            Sidebar end
        ***********************************-->

        <!--**********************************
            Content body start
        ***********************************-->


        <div class="content-body">
            <div class="container-fluid">
                <div class="row justify-content-center">
                    <div class="col-lg-8">
                        <div class="card">
                            <div class="card-body">
                                <div class="card-title">
                                    <h3>Available Course</h3>
                                </div>
                                <div class="table-responsive">
                                    <table class="table">
                                        <thead>
                                            <tr>
                                                <th>#</th>
                                                <th>Course Code</th>
                                                <th>Course Name</th>
                                                <th>No of Credit</th>
                                                <th>Elective Status</th>
                                                <th>No of Slots</th>
                                                <th>Available Status</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% courses.forEach(y=> { %>
                                                <tr>
                                                    <td>
                                                        <%= 1 %>
                                                    </td>
                                                    <td>
                                                        <%= y.coursecode %>
                                                    </td>
                                                    <td>
                                                        <%= y.coursename %>
                                                    </td>
                                                    <td>
                                                        <%= y.credit %>
                                                    </td>
                                                    <td>
                                                        <%=y.professional_elective_for %>
                                                    </td>
                                                    <td>
                                                        <%=y.slots %>
                                                    </td>
                                                    <td><i class="fa fa-circle-o text-success  mr-2"></i>Open</td>
                                                </tr>
                                                <% }) %>
                                                    <tr>
                                                        <td>2</td>
                                                        <td>21CSL25</td>
                                                        <td>Database Management Systems
                                                        </td>
                                                        <td>3</td>
                                                        <td>Professional</td>
                                                        <td>40</td>
                                                        <td><i class="fa fa-circle-o text-success  mr-2"></i>Open</td>
                                                    </tr>
                                                    <tr>
                                                        <td>3</td>
                                                        <td>21CSL22</td>
                                                        <td>Python Programming and Frameworks</span>
                                                        </td>
                                                        <td> 4</td>
                                                        <td>Open</td>
                                                        <td>50</td>
                                                        <td><i class="fa fa-circle-o text-danger  mr-2"></i>Closed</td>
                                                    </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>


                    <div class="col-lg-12">
                        <div class="card">
                            <div class="card-body">
                                <div class="form-validation">
                                    <form action="/api/courses/selectedcourses" method="post" class="form-valide">

                                        <div class="form-group row">
                                            <label class="col-lg-4 col-form-label" for="val-">Select Your Course<span
                                                    class="text-danger">*</span>
                                            </label>
                                            <div class="col-lg-6">
                                                <div class="form-row align-items-center">
                                                    <select class="custom-select mr-sm-2" id="option1" name="option1">
                                                        <option selected="selected">Choose...</option>
                                                        <% courses.forEach(y=> { %>
                                                            <option value="<%= y.coursecode%>">
                                                                <%= y.coursecode +" "+ y.coursename %>
                                                                    <% }) %>
                                                            </option>                                                            
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                                        <div class=" form-group row">
                                                                    <label class="col-lg-4 col-form-label" for="val-">
                                                                        Choose the Staff <span
                                                                            class="text-danger">*</span>
                                                                    </label>

                                                                    <div class="col-lg-6">
                                                                        <div class="form-row align-items-center">
                                                                            <select class="custom-select mr-sm-2"
                                                                                id="option2" name="option2">
                                                                                <option selected="selected">
                                                                                    Choose...</option>
                                                                                <!-- <option value="1" disabled>Dine</option> -->
                                                                                <% staffs.forEach(y=> { %>
                                                                                    <option value=<%=y.Staff_Name %> >
                                                                                        <%=y.Staff_Name %>
                                                                                    </option>

                                                                                    <% }) %>
                                                                                        <!-- <option value="3">Kav</option> -->
                                                                            </select>
                                                                        </div>
                                                                    </div>
                                                </div>

                                                <div class="form-group row">
                                                    <div class="col-lg-8 ml-auto">
                                                        <button class="btn btn-primary">Submit</button>
                                                    </div>
                                                </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- #/ container -->
        </div>
        <!--**********************************
            Content body end
        ***********************************-->


        <!--**********************************
            Footer start
        ***********************************-->
        <div class="footer">
            <div class="copyright">
                <p>Copyright &copy; Designed & Developed by <a href="https://themeforest.net/user/quixlab">Quixlab</a>
                    2018</p>
            </div>
        </div>
        <!--**********************************
            Footer end
        ***********************************-->
    </div>
    <!--**********************************
        Main wrapper end
    ***********************************-->

    <!--**********************************
        Scripts
    ***********************************-->
    <script src="plugins/common/common.min.js"></script>
    <script src="js/custom.min.js"></script>
    <script src="js/settings.js"></script>
    <script src="js/gleek.js"></script>
    <script src="js/styleSwitcher.js"></script>

</body>

</html>